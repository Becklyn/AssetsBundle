{
    "name": "becklyn/assets-bundle",
    "description": "High-performance asset handling for symfony projects.",
    "keywords": ["becklyn", "asset", "assets", "symfony"],
    "license": "BSD-3-Clause",
    "type": "symfony-bundle",
    "homepage": "https://github.com/Becklyn/BecklynAssetsBundle",
    "authors": [
        {
            "name": "Becklyn Studios",
            "homepage": "https://becklyn.com/"
        }
    ],
    "require": {
        "php": "^7.1",
        "symfony/cache": "^3.3|^4.0",
        "symfony/config": "^3.3|^4.0",
        "symfony/console": "^3.3|^4.0",
        "symfony/dependency-injection": "^3.3|^4.0",
        "symfony/filesystem": "^3.3|^4.0",
        "symfony/finder": "^3.3|^4.0",
        "symfony/http-kernel": "^3.3|^4.0",
        "symfony/routing": "^3.3|^4.0",
        "twig/twig": "^1.27|^2.1"
    },
    "conflict": {
        "symfony/asset": "*"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^3.3|^4.0"
    },
    "minimum-stability": "beta",
    "autoload": {
        "psr-4": {"Becklyn\\AssetsBundle\\": ""}
    },
    "autoload-dev": {
        "psr-4": {"Tests\\Becklyn\\AssetsBundle\\": "tests/"}
    }
}
