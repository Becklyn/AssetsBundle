{
    "name": "becklyn/assets-bundle",
    "type": "symfony-bundle",
    "description": "High-performance asset handling for symfony projects.",
    "keywords": [
        "becklyn",
        "asset",
        "assets",
        "symfony"
    ],
    "homepage": "https://github.com/Becklyn/AssetsBundle",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Becklyn Studios",
            "homepage": "https://becklyn.com/"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "symfony/cache": "^4.2",
        "symfony/config": "^4.2",
        "symfony/console": "^4.2",
        "symfony/contracts": "^1.0",
        "symfony/dependency-injection": "^4.2",
        "symfony/filesystem": "^4.2",
        "symfony/finder": "^4.2",
        "symfony/http-kernel": "^4.2",
        "symfony/process": "^4.2",
        "symfony/routing": "^4.2",
        "twig/twig": "^2.9"
    },
    "conflict": {
        "symfony/asset": "*"
    },
    "require-dev": {
        "becklyn/php-cs": "^1.1",
        "symfony/phpunit-bridge": "^4.2"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Becklyn\\AssetsBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Becklyn\\AssetsBundle\\": "tests/"
        }
    }
}
